{{- range .Blocks -}}
	{{- if eq .Kind "paragraph" -}}
		{{- exec "text" .Text | paragraph -}}
	{{- else if eq .Kind "code" -}}
		{{- exec "text" .Text | codeBlock -}}
	{{- else if eq .Kind "header" -}}
		{{- exec "text" .Text | header .Level -}}
	{{- else if eq .Kind "list" -}}
		{{- exec "text" .Text | paragraph -}}
	{{- end -}}
{{- end -}}

